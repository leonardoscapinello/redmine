{
  "name": "redmine",
  "version": "1.0.0",
  "description": "redmine integration with sysaid application",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "redmine-install": "sucrase-node ./serviceInstall.js",
    "redmine-uninstall": "sucrase-node ./serviceUnistall.js",
    "redmine-build": "sucrase ./src -d ./build --transforms javascript,imports",
    "redmine-start": "node ./build/server.js",
    "redmine-debug": "nodemon src/server.js",
